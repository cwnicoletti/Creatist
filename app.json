{
  "expo": {
    "name": "ExhibitU",
    "slug": "ExhibitU",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/ExhibitU_icon.png",
    "splash": {
      "image": "./assets/ExhibitU_icon_transparent_white_spaced.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exhibitu.ios",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "So you can add pictures to your ExhibitU profile",
        "UIViewControllerBasedStatusBarAppearance": "YES"
      }
    },
    "web": {
      "favicon": "./assets/ExhibitU_icon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "svgx"]
    },
    "sdkVersion": "42.0.0"
  }
}
